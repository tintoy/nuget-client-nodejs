{
  "name": "nuget-client",
  "version": "0.1.6",
  "description": "A NuGet API client for NodeJS.",
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/tintoy/nuget-client-nodejs.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc -p ./",
    "build:live": "./node_modules/.bin/tsc --watch -p ./",
    "prepare": "./node_modules/.bin/tsc -p ./",
    "docs": "./node_modules/.bin/typedoc --mode modules --excludePrivate --out ./docs",
    "test": "./node_modules/.bin/mocha -r ts-node/register test/**/*.test.ts"
  },
  "keywords": [
    "nuget",
    "client"
  ],
  "author": "tintoy",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "mz": "^2.6.0",
    "semver": "^5.4.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@types/chai": "^4.0.2",
    "@types/mocha": "^2.2.41",
    "@types/mz": "0.0.31",
    "@types/node": "^8.0.19",
    "@types/semver": "^5.3.33",
    "@types/xml2js": "^0.4.0",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "ts-node": "^3.3.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2"
  }
}
